    "homepage": "https://github.com/kayzorelabs/phpconfigparser",
    "version": "1.1.0",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Vítor Brandão",
            "email": "noisebleed@noiselabs.org"
        },
        {
            "name": "Kayzore Labs",
            "email": "kayzorelabs@gmail.com"
        }
    ],
    "support": {
